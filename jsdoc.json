{
  "source": {
    "include": ["./backend", "./frontend"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "opts": {
    "recurse": true,
    "destination": "./docs/",
    "template": "node_modules/docdash"
  },
  "templates": {
    "default": {
      "includeDate": false
    }
  }
}
